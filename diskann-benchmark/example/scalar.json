            "groundtruth": "disk_index_10pts_idx_uint32_truth_search_res.bin",
            "reps": 5,
            "num_threads": [
              1
            ],
            "runs": [
              {
                "search_n": 10,
                "search_l": [
                  10,
                  20,
                  30,
                  40
                ],
                "recall_k": 10
              }
            ]
          }
        },
        "num_bits": 1,
